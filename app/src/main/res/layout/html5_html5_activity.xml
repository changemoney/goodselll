<?xml version="1.0" encoding="utf-8"?>
<FrameLayout android:id="@id/layout_webview_container" android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="fill_parent">
        <include layout="@layout/default_titlebar" />
        <RelativeLayout android:layout_width="fill_parent" android:layout_height="fill_parent">
            <com.ganji.android.comp.html5.WebViewWrapper android:id="@id/webview" android:layout_width="fill_parent" android:layout_height="fill_parent" />
            <include layout="@layout/default_loading_container" />
        </RelativeLayout>
    </LinearLayout>
</FrameLayout>