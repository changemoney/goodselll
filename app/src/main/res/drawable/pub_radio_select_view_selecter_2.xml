<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <stroke android:width="1.0px" android:color="@color/green_39bc30" />
            <corners android:radius="30.0dip" />
            <solid android:color="@color/green_39bc30" />
        </shape>
        <color android:color="@color/white" />
    </item>
    <item android:state_selected="false">
        <shape>
            <stroke android:width="1.0px" android:color="@color/gray_f3f3f3" />
            <corners android:radius="30.0dip" />
            <solid android:color="@color/gray_f3f3f3" />
        </shape>
        <color android:color="@color/gray_999999" />
    </item>
</selector>