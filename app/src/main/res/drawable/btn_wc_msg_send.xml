<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#ff2e9626" />
            <corners android:radius="17.5dip" />
            <size android:height="35.0dip" android:width="60.0dip" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/white" />
            <stroke android:width="1.0px" android:color="@color/divider_line" />
            <corners android:radius="17.5dip" />
            <size android:height="35.0dip" android:width="60.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#ff39bc30" />
            <corners android:radius="17.5dip" />
            <size android:height="35.0dip" android:width="60.0dip" />
        </shape>
    </item>
</selector>